# Copy this file to .env and fill in your credentials.
# Files named .env are ignored by git (see .gitignore).

# AskNews credentials
ASKNEWS_API_ID=your_asknews_api_id
ASKNEWS_API_KEY=your_asknews_api_key
ASKNEWS_BASE_URL=https://api.asknews.app/v1/news/search

# ChatGPT / OpenAI credentials
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_RESEARCH_MODEL=gpt-4o-mini
OPENAI_RESEARCH_MAX_ITEMS=20
OPENAI_SYNTH_SEED_MODEL=gpt-4.1-mini
OPENAI_SYNTH_SIFTER_MODEL=gpt-4o
OPENAI_SYNTH_ANALYST_MODEL=gpt-4o-mini
OPENAI_SYNTH_MAX_ITEMS=12
OPENAI_KEYWORD_MODEL=gpt-4o-mini
OPENAI_KEYWORD_CACHE_SIZE=64

# AEI tuning (optional)
AEI_MAX_API_CALLS_PER_RUN=6
AEI_ASKNEWS_CACHE_TTL_MIN=5
AEI_ASKNEWS_MIN_INTERVAL_SEC=1.0
AEI_NEWS_PROVIDER=hybrid

# TextQL search (optional but recommended)
TEXTQL_API_KEY=your_textql_api_key
TEXTQL_BASE_URL=https://app.textql.com/v1
TEXTQL_API_MODE=RPC
TEXTQL_MODEL=MODEL_SONNET_4
TEXTQL_PARADIGM_TYPE=TYPE_UNIVERSAL
TEXTQL_PARADIGM_VERSION=1
TEXTQL_ENABLE_WEB_SEARCH=true
TEXTQL_ENABLE_PYTHON=true
TEXTQL_ENABLE_SQL=false
TEXTQL_ENABLE_ONTOLOGY=false
TEXTQL_ENABLE_EXPERIMENTAL=false
TEXTQL_ENABLE_TABLEAU=false
TEXTQL_ENABLE_AUTO_APPROVE=false
TEXTQL_ENABLE_STREAMLIT=false
TEXTQL_ENABLE_GOOGLE_DRIVE=false
TEXTQL_ENABLE_POWERBI=false
TEXTQL_RESEARCH_MODE=false
TEXTQL_SQL_CONNECTOR_ID=
TEXTQL_TIMEOUT_SEC=120.0
TEXTQL_MAX_RETRIES=2
TEXTQL_RETRY_DELAY_SEC=5.0
TEXTQL_RPC_URL=https://app.textql.com/rpc/public/textql.rpc.public.chat.ChatService/QueryOneShot
TEXTQL_ANSWER_URL=https://app.textql.com/rpc/public/textql.rpc.public.chat.ChatService/GetAPIChatAnswer
TEXTQL_POLL_INTERVAL_SEC=3.0
TEXTQL_POLL_MAX_ATTEMPTS=40
TEXTQL_POLL_MAX_DURATION_SEC=0
TEXTQL_POLL_BACKOFF_MULTIPLIER=1.25
